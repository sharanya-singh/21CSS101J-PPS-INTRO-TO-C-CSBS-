# include<stdio.h>
int main()
{
    printf("RA2211042010010"\n);
    float km,m,cm,ft,inch ;
    printf( "\nEnter the distance in Kilometers: " ) ;
    scanf("%f", &km ) ;
    m = km*1000;
    cm=m*100;
    inch=cm/2.54;
    ft=inch/12 ;
    printf ( "Distance in meters = %f\n",m );
    printf ( "Distance in centimeter = %f\n",m );
    printf ( "Distance in feet = %f\n",m );
    printf ( "Distance in inches = %f\n",m );
    return 0;
   }
