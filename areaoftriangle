#include<stdio.h>
int main()
{ 
   printf("RA2211042010010\n");
   float a,b,c,sp,area;
   printf( "\nEnter the sides of a triangle: ");
   scanf( "%f %f %f", &a,&b,&c );
   sp = ( a+b+c) /2 ;
   area = sqrt(sp * (sp-a) * (sp-b) * (sp-c) );
   printf( "Area of a triangle is: %f", area);
    return 0;
}
